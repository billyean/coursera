/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel, out=Notsel);
    And(a=Notsel, b=a, out=NotselAnda);
    And(a=sel, b=b, out=selAndb);
    Or(a=NotselAnda, b=selAndb, out=out);
}